{
    "name": "codetoad/strava",
    "description": "A laravel package to access data from the Strava API.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.1.0",
        "illuminate/support": "5.1.* | 5.2.* | 5.3.* | 5.4.* | 5.5.* | 5.6.* | 5.7.* | 5.8.* | ^6.0 | ^7.0 | ^8.0"
    },
    "autoload": {
        "psr-4": {
            "CodeToad\\Strava\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeToad\\Strava\\StravaServiceProvider"
            ],
            "aliases": {
                "Strava": "CodeToad\\Strava\\StravaFacade"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Richie McMullen",
            "email": "richiemcmullen007@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
